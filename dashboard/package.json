{
  "name": "inposter-ai-dashboard",
  "version": "1.0.0",
  "description": "Local analytics dashboard for InposterAI browser extension",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-global": "npm install -g nodemon",
    "populate-data": "node scripts/populate-dummy-data.js",
    "populate-clear": "node scripts/populate-dummy-data.js --clear",
    "populate-custom": "node scripts/populate-dummy-data.js --count"
  },
  "keywords": [
    "ai-detection",
    "analytics",
    "dashboard",
    "express"
  ],
  "author": "InposterAI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
